{
  "manifest_version": 2,
  "name": "source-mapper",
  "description": "Fetches source maps for files",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script/background_script.js"
    ]
  },
  "devtools_page": "devtools/devtools-page.html",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ]
}